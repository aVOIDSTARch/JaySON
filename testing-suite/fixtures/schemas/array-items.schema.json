{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "ItemList",
    "description": "A list of items for testing array validation",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "integer"
            },
            "value": {
                "type": "string"
            }
        },
        "required": ["id"]
    },
    "minItems": 1,
    "maxItems": 100
}
