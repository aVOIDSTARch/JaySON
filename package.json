{
  "name": "@casinelli/jayson",
  "version": "1.0.1",
  "description": "A TypeScript utility for working with JSON schemas, validating data, and creating JSON files.",
  "keywords": [
    "json",
    "schema",
    "validation",
    "typescript"
  ],
  "homepage": "https://github.com/aVOIDSTARch/JaySON#readme",
  "bugs": {
    "url": "https://github.com/aVOIDSTARch/JaySON/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aVOIDSTARch/JaySON.git"
  },
  "license": "MIT",
  "author": "aVOIDSTARch",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "main": "dist/cjs/index.js",
  "types": "dist/esm/index.d.ts",
  "bin": {
    "jayson": "dist/esm/cli.js"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:watch": "tsc -p tsconfig.esm.json --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run testing-suite/unit",
    "test:integration": "vitest run testing-suite/integration",
    "test:cli": "vitest run testing-suite/cli",
    "test:all": "npm run build && npm run test",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "dependencies": {
    "argparse": "^2.0.1",
    "assertion-error": "^2.0.1",
    "ast-v8-to-istanbul": "^0.3.9",
    "balanced-match": "^1.0.2",
    "brace-expansion": "^2.0.2",
    "chai": "^6.2.1",
    "debug": "^4.4.3",
    "entities": "^4.5.0",
    "es-module-lexer": "^1.7.0",
    "esbuild": "^0.27.2",
    "estree-walker": "^3.0.3",
    "expect-type": "^1.3.0",
    "fdir": "^6.5.0",
    "fsevents": "^2.3.3",
    "has-flag": "^4.0.0",
    "html-escaper": "^2.0.2",
    "istanbul-lib-coverage": "^3.2.2",
    "istanbul-lib-report": "^3.0.1",
    "istanbul-lib-source-maps": "^5.0.6",
    "istanbul-reports": "^3.2.0",
    "js-tokens": "^9.0.1",
    "linkify-it": "^5.0.0",
    "lunr": "^2.3.9",
    "magic-string": "^0.30.21",
    "magicast": "^0.5.1",
    "make-dir": "^4.0.0",
    "markdown-it": "^14.1.0",
    "mdurl": "^2.0.0",
    "minimatch": "^9.0.5",
    "ms": "^2.1.3",
    "nanoid": "^3.3.11",
    "obug": "^2.1.1",
    "pathe": "^2.0.3",
    "picocolors": "^1.1.1",
    "picomatch": "^4.0.3",
    "postcss": "^8.5.6",
    "punycode.js": "^2.3.1",
    "rollup": "^4.54.0",
    "semver": "^7.7.3",
    "siginfo": "^2.0.0",
    "source-map-js": "^1.2.1",
    "stackback": "^0.0.2",
    "std-env": "^3.10.0",
    "supports-color": "^7.2.0",
    "tinybench": "^2.9.0",
    "tinyexec": "^1.0.2",
    "tinyglobby": "^0.2.15",
    "tinyrainbow": "^3.0.3",
    "uc.micro": "^2.1.0",
    "undici-types": "^7.16.0",
    "vite": "^7.3.0",
    "why-is-node-running": "^2.3.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "typedoc": "^0.28.15",
    "typescript": "^5.3.0",
    "vitest": "^4.0.16"
  },
  "module": "dist/esm/index.js"
}
